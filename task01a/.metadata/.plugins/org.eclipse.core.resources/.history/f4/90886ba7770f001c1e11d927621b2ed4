
public class Test01 {
	public static void main(String[] args) {
		double x=0.0;
		//1.
		x=(4.0/5.0-3.0/7.0)*35.0;
		System.out.printf("1. %.2f\n"+x);
		//2.
		x=2.0+6.0/11.0-(6.0/11.0+5.0/12.0);
		System.out.println("2. "+x);
		//3.
		x=65.0*(9.0/13.0+2.0/5.0);
		System.out.println("3. "+x);
		//4.
		x=2.0+6.0/11.0-(6.0/11.0-5.0/12.0);
		System.out.println("4. "+x);
		//5.
		x=0.2*2.0/13.0+1.0/5.0*11.0/13.0;
		System.out.println("5. "+x);
		//6.
		x=4.0/5.0*(1.0+5.0/17.0)-0.8*5.0/17.0;
		System.out.println("6. "+x);
	}

}
