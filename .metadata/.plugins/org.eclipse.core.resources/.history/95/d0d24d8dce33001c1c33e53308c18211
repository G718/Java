import java.util.*;
public class Test12 {

	public static void main(String[] args) {
		
		int i,j,k;
		String s1,s2,s3;
		char c1,c2;
		Scanner scanner=new Scanner(System.in);
		System.out.println("请输入第一串字符");
		s1=scanner.nextLine();
		System.out.println("请输入第二串字符");
		s2=scanner.nextLine();
		if(s1.length()<s2.length()){
			s3=s1;
			s1=s2;
			s2=s3;
		}
		System.out.println(s1);
		System.out.println(s2);
		
	}
	
}
