import java.util.*;
public class Test02 {

	public static void main(String[] args) {
		
		int i,j=0,k=0;
		char c,e;
		String s="11cfbks34cdf2ghacdf11";
		String q="11";
		for(i=0;i<s.length();i++){
			c=s.charAt(i);
			e=q.charAt(j);
			while(c==e){
				c=s.charAt(++i);
				e=q.charAt(++j);
				if(j==q.length()-1){
					k++;
					j=0;
					break;
				}
			}
		}
		System.out.println(q+"出现次数为："+k);
		
	}
	
}
