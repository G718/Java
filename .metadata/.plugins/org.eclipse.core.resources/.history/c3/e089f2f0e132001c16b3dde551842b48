import java.util.*;
public class Test02 {

	public static void main(String[] args) {
		
		int i,j=0,k=0;
		char c,e;
		String s="11cfbks34cdf2ghacdf11";
		String q="cdf";
		for(i=0;i<s.length();i++){
			c=s.charAt(i);
			e=q.charAt(j);
			while(c==e){
				j++;
				if(j==q.length()-2){
					k++;
					j=0;
					break;
				}
			}
//			System.out.print("\n"+c+":");
//			for(;j<q.length();j++){
//				
