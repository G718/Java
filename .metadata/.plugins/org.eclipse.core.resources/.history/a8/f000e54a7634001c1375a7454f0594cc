import java.util.*;
public class Test11 {

    public static void main(String[] args) {

        int i = 0, k = 0, l = 1;//l£ºµ¥´ÊÊý
        int j=0,j1=0,j2=0;
        String s;
        char c;
        char d,d1,d2;
        String a = "and";
        String f = "for";
        String t = "the";
        char L[] = new char[10];
        Scanner scanner = new Scanner(System.in);
        s = scanner.nextLine();
        s=s+" ";
        for (i = 0; i < s.length(); i++) {
            c = s.charAt(i);
            if (c!=32) {
                k++;
                //and
                if (j < a.length()) {
                    d = a.charAt(j++);
                    if (d != c) {
                        j = 0;
                    }
                    if (j == 3&&k<3) {
                        k = 0;
                        j = 0;
                    }
                }
                //for
                if(j1<f.length()){
                    d1=f.charAt(j1++);
                    if(d1!=c){
                        j1=0;
                    }
                    if(j1==3){
                        k=0;
                        j1=0;
                    }
                }
                //the
                if(j2<t.length()){
                    d2=t.charAt(j2++);
                    if(d2!=c){
                        j2=0;
                    }
                    if(j2==3){
                        k=0;
                        j2=0;
                    }
                }
            } else {
                if (k > 2) {
                    L[l - 1] = s.charAt(i - k);
                    l++;
                }
                k = 0;
            }
        }
        for (i = 0; i < l-1; i++) {
            if(L[i]>97)
                L[i]=(char)(L[i]-32);
            System.out.print(L[i]);
        }

    }
}